<plugin name='ooo' version='1.6.10' grailsVersion='1.3.1 &gt; *'>
  <author>Ralf Bensmann</author>
  <authorEmail>grails@bensmann.com</authorEmail>
  <title>OOo for Grails.</title>
  <description>Generate documents using OpenOffice.org and maintain any type of documents through a document service.</description>
  <documentation>http://grails.org/ooo+Plugin</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>Config</resource>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>com.bensmann.ooo.OooDocumentController</resource>
    <resource>com.bensmann.ooo.OooDocument</resource>
    <resource>com.bensmann.ooo.OooDocumentService</resource>
    <resource>com.bensmann.ooo.OooService</resource>
    <resource>com.bensmann.ooo.OooTagLib</resource>
    <resource>com.bensmann.ooo.ControllerHelper</resource>
  </resources>
  <dependencies>
    <plugin name='controllers' version='1.3.1' />
    <plugin name='core' version='1.3.1' />
    <plugin name='hibernate' version='1.3.1' />
    <plugin name='glue' version='1.3.0 &gt; *' />
  </dependencies>
  <behavior />
</plugin>